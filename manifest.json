{
  "manifest_version": 3,
  "name": "Data Scrapper",
  "version": "1.0",
  "description": "Scrap web data",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": {
      "192": "images/logo192.png",
      "512": "images/logo512.png"
    },
    "default_popup": "popup.html",
    "default_title": "Click to Scrape Data"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
